package controller;
import java.util.LinkedList;
import javax.xml.bind.annotation.XmlRootElement;
import javax.xml.bind.annotation.XmlElement;
import java.util.ArrayList;

@XmlRootElement(name = "resume")
public class Curriculum {
    private String titre;
    private  information_perso infos;
    private ArrayList<experience_pro> experiences_pro;
    private ArrayList<competence> competences;
    private ArrayList<langue> langues;

    public Curriculum() {
        this.titre = new String();
        this.infos = new information_perso();
        this.experiences_pro = new ArrayList<experience_pro>();
        this.langues = new ArrayList <langue>();
        this.competences = new ArrayList<competence>();
    }
    /*public Curriculum(String titre, information_perso info) {
        this.titre = titre;
        this.infos = info;
    }*/

    public Curriculum(String titre, information_perso info, ArrayList<experience_pro> experiences_pr, ArrayList<competence> competence, ArrayList<langue> langue) {
        this.titre = titre;
        this.infos = info;
        this.experiences_pro = experiences_pr;
        this.langues = langue;
        this.competences = competence;
    }


    public String getTitre() {
        return titre;
    }


    public void setTitre(String titre) {
        this.titre = titre;
    }

    public information_perso getInfo_persos() {
        return infos;
    }


    public void setInfo_persos(information_perso info_persos) {
        this.infos = info_persos;
    }

    public ArrayList<experience_pro> getExperiences_pro() {
        return experiences_pro;
    }


    public void setExperiences_pro(ArrayList<experience_pro> Experiences_pro) {
        this.experiences_pro = Experiences_pro;
    }

    public ArrayList<competence> getCompetences() {
        return competences;
    }


    public void setCompetences(ArrayList<competence> compt) {
        this.competences = compt;
    }

    public ArrayList<langue> getLangues() {
        return langues;
    }


    public void setLangues(ArrayList<langue> lang) {
        this.langues = lang;
    }
}
